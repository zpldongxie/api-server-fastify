{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": [0, "error", "windows"],
        "max-len": ["error", 120],
        "object-curly-spacing": ["error", "always"],
        "new-cap": [0]
    }
}
