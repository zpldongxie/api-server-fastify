import Sequelize from 'sequelize'
import getSchema from './schemas/%title%.js'

const { Model } = Sequelize

class %title_U% extends Model {
  /**
   * 模型初始化
   *
   * @static
   * @param {*} sequelize
   * @param {*} XXDM 学校代码
   * @return {*} 
   * @memberof User
   */
   static init(sequelize, XXDM) {
    return super.init(getSchema(XXDM), {
      tableName: "%title%",
      sequelize,
      comment: '%title_CN%',
    })
  }

  static associate(models) {
    //No asociations
  }

  static getById(id) {
    return this.findOne({ where: { id } })
  }
}

export default %title_U%;
